{
  ".grow": {
    "&-relative": {
      "height": null,
      "width": null,
      "flexProps": {
        "flexGrow": 1
      }
    },
    "&-absolute": {
      "flexProps": {
        "positionType": "ABSOLUTE"
      },
      "height": null,
      "width": null,
      "left": 0,
      "right": 0,
      "bottom": 0,
      "top": 0
    }
  },
  ".padding": {
    "&-vertical": {
      "paddingBottom": 15,
      "paddingTop": 15
    },
    "&-horizontal": {
      "paddingLeft": 15,
      "paddingRight": 15
    }
  },
  ".margin": {
    "&-vertical": {
      "marginBottom": 15,
      "marginTop": 15
    },
    "&-horizontal": {
      "marginLeft": 15,
      "marginRight": 15
    }
  },
  ".shadow": {
    "ios": {
      "shadowOffset": {
        "x": 2,
        "y": 3
      },
      "shadowRadius": 1,
      "shadowOpacity": 0.4,
      "masksToBounds": false
    },
    "android": {
      "elevation": 5
    }
  },
  ".hidden": {
    "width": 0,
    "height": 0,
    "marginBottom": 0,
    "marginTop": 0,
    "marginLeft": 0,
    "marginRight": 0,
    "padding": 0,
    "paddingBottom": 0,
    "paddingLeft": 0,
    "paddingRight": 0,
    "paddingTop": 0
  },
  ".absolute": {
    ".topleft": {
      "height": null,
      "left": 0,
      "top": 0
    },
    ".topright": {
      "height": null,
      "top": 0,
      "right": 0
    },
    ".bottomleft": {
      "height": null,
      "bottom": 0,
      "left": 0
    },
    ".bottomright": {
      "height": null,
      "bottom": 0,
      "right": 0
    },
    "flexProps": {
      "positionType": "ABSOLUTE"
    }
  },
  ".centerChild": {
    "flexProps": {
      "justifyContent": "CENTER"
    }
  },
  "#search": {
    "flexProps": {
      "flexGrow": 1,
      "positionType": "RELATIVE",
      "alignSelf": "STRETCH",
      "alignItems": "CENTER",
      "justifyContent": "FLEX_START",
      "flexDirection": "ROW",
      "alignContent": "CENTER"
    }
  },
  "#mainPage": {
    "&-list": {
      "flexProps": {
        "flexGrow": 5
      }
    },
    "&-profileInfo": {
      "flexProps": {
        "flexGrow": 5,
        "flexDirection": "ROW"
      }
    },
    "&-searchButton": {
      "flexProps": {
        "flexGrow": 1,
        "alignSelf": "CENTER",
        "flexShrink": null,
        "positionType": "RELATIVE"
      },
      "textColor": "rgba( 0, 0, 0, 1 )",
      "font": {
        "name": "",
        "family": "FontAwesome5BrandsRegular",
        "style": null,
        "bold": false,
        "italic": false,
        "size": 30
      },
      "ellipsizeMode": "MIDDLE"
    },
    "&-profilePhoto": {
      "flexProps": {
        "flexGrow": 1
      },
      "borderRadius": null,
      "borderWidth": null,
      "borderColor": "rgba( 255, 255, 255, 0 )",
      "alpha": 1,
      "backgroundColor": "rgba( 189, 16, 224, 1 )"
    },
    "&-profileName": {
      "flexProps": {
        "flexGrow": 3
      },
      "backgroundColor": "rgba( 0, 0, 0, 0 )",
      "textColor": "rgba( 0, 0, 0, 1 )",
      "ellipsizeMode": "END",
      "font": {
        "name": "",
        "family": "SFProText",
        "style": "Regular",
        "bold": false,
        "italic": false
      },
      "width": null,
      "height": null,
      "marginLeft": 10
    },
    "&-headers": {
      "textColor": "rgba( 0, 0, 0, 1 )",
      "font": {
        "name": "",
        "family": "FontAwesome5BrandsRegular",
        "style": null,
        "bold": false,
        "italic": false
      }
    },
    "&-bookList": {
      "flexProps": {
        "flexGrow": 1
      }
    }
  },
  "#header": {
    "visible": false
  },
  "#detailsPage": {
    "&-bookInfo": {
      "flexProps": {
        "flexGrow": 9,
        "alignItems": "STRETCH",
        "direction": "INHERIT",
        "justifyContent": "CENTER",
        "alignContent": "CENTER",
        "flexWrap": "NOWRAP"
      }
    },
    "&-description": {
      "flexProps": {
        "flexGrow": 4,
        "alignSelf": "AUTO"
      },
      "textAlignment": "MIDCENTER",
      "font": {
        "name": "",
        "family": "FontAwesome5BrandsRegular",
        "style": null,
        "bold": false,
        "italic": false
      }
    },
    "&-buttons": {
      "flexProps": {
        "flexGrow": 1,
        "flexDirection": "ROW",
        "alignItems": "CENTER",
        "justifyContent": "CENTER"
      }
    },
    "&-purchaseButton": {
      "flexProps": {
        "flexGrow": 1
      },
      "alpha": 1,
      "backgroundColor": {
        "normal": "rgba( 0, 0, 0, 1 )"
      },
      "borderRadius": 25
    }
  },
  "#bookPage": {
    "&-header": {
      "titleColor": "rgba( 0, 0, 0, 1 )",
      "backgroundColor": "rgba( 0, 0, 0, 0 )"
    },
    "&-author": {
      "flexProps": {
        "flexGrow": 1,
        "alignSelf": "STRETCH",
        "positionType": "RELATIVE"
      },
      "textColor": "rgba( 0, 0, 0, 0.79 )",
      "font": {
        "name": "",
        "family": "Default",
        "style": "Regular",
        "bold": false,
        "italic": false
      },
      "textAlignment": "MIDCENTER"
    },
    "&-image": {
      "flexProps": {
        "flexGrow": 20
      }
    },
    "&-bookName": {
      "flexProps": {
        "flexGrow": 1
      },
      "textColor": "rgba( 0, 0, 0, 1 )",
      "font": {
        "name": "",
        "family": "Default",
        "style": "Bold",
        "bold": true,
        "italic": false
      },
      "textAlignment": "MIDCENTER"
    },
    "&-point": {
      "flexProps": {
        "flexGrow": 1
      },
      "textAlignment": "MIDCENTER",
      "font": {
        "name": "",
        "family": "Default",
        "style": "Bold",
        "bold": true,
        "italic": false
      },
      "textColor": "rgba( 0, 0, 0, 1 )"
    },
    "&-detailButtons": {
      "flexProps": {
        "flexGrow": 1
      },
      "borderRadius": 25,
      "textColor": "rgba( 0, 0, 0, 1 )",
      "backgroundColor": {
        "normal": "rgba( 255, 255, 255, 1 )"
      },
      "margin": 5,
      "font": {
        "name": "",
        "family": "FontAwesome5BrandsRegular",
        "style": null,
        "bold": false,
        "italic": false
      }
    }
  }
}